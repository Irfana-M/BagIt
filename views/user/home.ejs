<!-- Start Product Area -->
<section class="product-area section_gap">
    <div class="container">
        <!-- Latest Products -->
        <div class="row justify-content-center">
            <div class="col-lg-6 text-center">
                <div class="section-title">
                    <h1>Latest Products</h1>
                    <p>"Bagit Shopping brings you trendy, stylish, and functional bags for every occasion.âœ¨ Carry elegance, flaunt confidence, and bag your perfect look! ðŸ‘œ"</p>
                </div>
            </div>
        </div>
        <div class="row">
            <% for(let i=0; i<products.length; i++) { %>
            <!-- Single product -->
            <div class="col-lg-3 col-md-6">
                <div class="single-product">
					<a href="/productDetails?id=<%=products[i]._id%>">
                    <img class="img-fluid" src="/uploads/product-images/<%=products[i].productImage[0]%>" alt="<%=products[i].productName%>">
                    <div class="product-details">
                            <h6><%=products[i].productName%></h6>
                        </a>
                        <div class="price">
                            <h6><%=products[i].salePrice%></h6>
                            <% if (products[i].salePrice < products[i].regularPrice) { %>
                                <h6 class="l-through">â‚¹<%= products[i].regularPrice %></h6>
                            <% } %>
                        </div>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</section>
<!-- End Product Area -->

